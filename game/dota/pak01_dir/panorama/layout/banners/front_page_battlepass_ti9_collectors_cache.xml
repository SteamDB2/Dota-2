<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/frontpage_shared.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/international2019/bp_international2019_shared.vcss_c" />
		<include src="s2r://panorama/styles/banners/front_page_battlepass_ti9_collectors_cache.vcss_c" />
	</styles>

	<CustomLayoutPanel onactivate="DOTAShowItemDefPage( 13080 )">
		<Panel class="FrontpageCellImage" />
		
		<DOTACountdown id="Countdown" end-time="1565654400" server-time="true">
			<Label class="time-left" text="{t:d:t:T:countdown_time}" />
			<Label class="remaining" text="#DOTA_TI9_Collectors_Cache_Remaining" />
			<Label class="finished" text="#DOTA_TI9_Collectors_Cache_Finished" />
		</DOTACountdown>

		<Label class="FrontpageCellHeader" text="#FrontPage_New_Treasure_Header" />

		<Panel id="LevelBadge">
			<Label id="BadgeLabel" text="#DOTA_TI9_Collectors_Cache_BadgeLabel" />
			<Panel id="BadgeContent">
				<Label id="36Label" text="#DOTA_TI9_Collectors_Cache_Badge31" />
				<Label id="LevelLabel" text="#DOTA_TI9_Collectors_Cache_BadgeLevels" />
			</Panel>
		</Panel>

		<Panel class="content">
			<Label class="treasure-vote-title" text="#DOTA_TI9_Collectors_Cache2_Title" />
			<Label class="treasure-vote-desc" text="#DOTA_TI9_Collectors_Cache2_Desc" />
		</Panel>	
	</CustomLayoutPanel>
 
</root>
