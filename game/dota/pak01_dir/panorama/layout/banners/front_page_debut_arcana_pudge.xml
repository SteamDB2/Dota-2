<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/frontpage_shared.vcss_c" />
		<include src="s2r://panorama/styles/banners/front_page_debut_arcana_pudge.vcss_c" />
	</styles>

	<CustomLayoutPanel onactivate="DOTAShowReferencePage( file://{resources}/layout/leaf_pages/dashboard_page_debut_pudge_arcana.xml )">
		<Panel id="MainContainer" hittest="true">
			<DOTAScenePanel id="ModelBackground" map="backgrounds/hero_showcase_arcana_pudge_2cells_bg" camera="hero_camera" light="light_hero" renderdeferred="true" rotateonmousemove="true" yawmin="-0" yawmax="0" pitchmin="-0" pitchmax="0" antialias="true" particleonly="false"/>
			<DOTAScenePanel id="Model_FG" hittest="false" map="backgrounds/hero_showcase_arcana_pudge_2cells" camera="hero_camera" light="light_hero"  renderdeferred="true" deferredalpha="true" rotateonmousemove="true" yawmin="-2" yawmax="2" pitchmin="-2" pitchmax="2" antialias="true" particleonly="false"/>
		</Panel>

		<Label class="FrontpageCellHeader" text="#Debut_Arcana_Pudge_Title_Front_Page"/>
		<Panel class="FrontpageCellFooter">
			<Label class="FrontpageCellText" text="#Debut_Arcana_Pudge_Lore_FrontPage" />
			<Label class="FrontpageCellTitle" text="#Debut_Arcana_Pudge_Title_Front_Page_foot_title" />
		</Panel>
	</CustomLayoutPanel>
</root>
