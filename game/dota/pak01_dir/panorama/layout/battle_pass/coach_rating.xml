<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/current_battle_pass.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/coach_rating.vcss_c" />
	</styles>

	<snippets>
		<snippet name="RatingProgress">
			<Panel class="RatingProgress">
				<CircularProgressBar id="RatingProgressBar" />
				<Label class="RatingBadgeValue" text="{d:coach_rating}" />
			</Panel>
		</snippet>
		<snippet name="RatingBadge">
			<Panel class="RatingBadge">
				<Label class="RatingBadgeValue" text="{d:coach_rating}" />
			</Panel>
		</snippet>
		<snippet name="RatingIcon">
			<Panel class="RatingIcon">

			</Panel>
		</snippet>
	</snippets>

	<Panel>
		<!-- One of the snippets will be loaded based on the -->
	</Panel>
</root>
