<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/international2017/bp_international2017_shared.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/international2017/dota_hud_quest_status.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/quest_ranks.vcss_c" />
	</styles>

	<DOTAQuestStatus class="QuestStatusHidden" hittest="false">
		<Panel id="QuestStatusContainer" hittest="false" class="TopBottomFlow">
			<Panel id="QuestHeader">
				<Button id="QuestExpandButton1" class="QuestExpandButton">
					<Panel class="CheckIcon QuestStatusIcon"/>
					<Panel class="XIcon QuestStatusIcon"/>
				</Button>
				<Label id="PrimaryProgress" class="ProgressNumbers" text="{d:primary_progress} / {d:primary_goal}" hittest="false"/>
			</Panel>
			
		</Panel>
		
	</DOTAQuestStatus>
</root>


