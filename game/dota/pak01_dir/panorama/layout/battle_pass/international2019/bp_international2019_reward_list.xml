					<Panel class="DotaPlusIconLarge" onmouseover="UIShowTextTooltip( #DOTA_TI9_BattlePass_PlusReward )" onmouseout="UIHideTextTooltip()" />
				</Panel>
				<TextButton id="ClaimButton" text="#DOTA_Claim" />
			</Panel>
		</snippet>

