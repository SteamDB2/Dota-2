<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/hudstyles.vcss_c" />
		<include src="s2r://panorama/styles/hud/dota_hud_es_arcana_combo.vcss_c" />
	</styles>
	<DOTAHUDESArcanaCombo hittest="false" hittestchildren="false">
			<Panel id="ComboCountContainer">
				<Panel id="ComboDetail">
					<DOTAScenePanel id="ComboAmbientFX" hittest="false" camera="camera" renderdeferred="false" rendershadows="false" particleonly="true" map="scenes/hud/ui_es_arcana_combo_ambient"/>
				</Panel>
				<Panel id="ComboCountContents">
					<!--<Label id="ComboCountValue" class="ComboCountText" text="#DOTA_EarthshakerArcana_Hud_ComboCount"/>-->
					<Panel id="ComboContain" class="ComboDigits DigitContainer">
						<Panel class="ComboX ComboCountX ComboCountValue"/>
						<Panel id="ComboCountDigit1" class="ComboCountDigit ESDigit ComboCountValue"/>
						<Panel id="ComboCountDigit2" class="ComboCountDigit ESDigit ComboCountValue"/>
						<Panel id="ComboCountDigit3" class="ComboCountDigit ESDigit ComboCountValue"/>
					</Panel>
					<Label class="ComboCountLabel" text="#DOTA_EarthshakerArcana_Hud_ComboTitle"/>
				</Panel>
			</Panel>

			<Panel id="ComboSummaryContainer" >
				<DOTAScenePanel id="ComboSummaryFX" hittest="false" camera="camera" renderdeferred="false" rendershadows="false" particleonly="true" map="scenes/hud/ui_es_arcana_combo_summary"/>
				<Panel id="ComboDetail2">
					 
				</Panel>
				<Panel id="ComboSummaryContents">
					<Panel class="ComboSummary">
						<Panel class="DigitContainer">
							<Panel class="ComboX ComboSummaryX"/>
							<Panel id="ComboSummaryDigit1" class="ComboSummaryDigit ESDigit"/>
							<Panel id="ComboSummaryDigit2" class="ComboSummaryDigit ESDigit"/>
							<Panel id="ComboSummaryDigit3" class="ComboSummaryDigit ESDigit"/>
						</Panel>
						<Label class="SummaryComboLabel" text="#DOTA_EarthshakerArcana_Hud_ComboTitle"/>
					</Panel>

					<Panel class="DamageSummary">
						<Panel  id="DamageContain" class="DigitContainer">
							<Panel id="DamageSummaryDigit1" class="DamageSummaryDigit ESDigit"/>
							<Panel id="DamageSummaryDigit2" class="DamageSummaryDigit ESDigit"/>
							<Panel id="DamageSummaryDigit3" class="DamageSummaryDigit ESDigit"/>
							<Panel id="DamageSummaryDigit4" class="DamageSummaryDigit ESDigit"/>
							<Panel id="DamageSummaryDigit5" class="DamageSummaryDigit ESDigit"/>
						</Panel>
						<Label class="SummaryDamageLabel" text="#DOTA_EarthshakerArcana_Hud_ComboSummaryDamageLabel"/>
					</Panel>
				</Panel>
			</Panel>

	</DOTAHUDESArcanaCombo>
</root>
