<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/hud/dota_hud_game_end.vcss_c" />
	</styles>

	<DOTAGameEnd hittest="true" >

		<Panel id="WinLabelContainer" hittest="false">
			<DOTAScenePanel id="VictoryBackgroundScene" map="backgrounds/accept_popup_test" camera="shot_camera" hittest="false" particleonly="true"/>
			<Label id="VictoryLabel" class="TitleFont" text="{s:victory_title}" hittest="false"/>
		</Panel>

		<Panel id="TeamChallengeImage" hittest="false"/>
		<Label id="TeamChallengeTitle" class="TitleFont" text="#DOTA_TeamChallenge_Title" hittest="false"/>
		<Label id="TeamChallengeLabel" class="TitleFont" text="{s:team_challenge_result_text}" hittest="false"/>
		<Label id="TeamChallengeReward" class="TitleFont" text="#DOTA_TeamChallenge_Reward" hittest="false" />

		<Button id="ContinueButton" class="LargeButton ButtonBevel" onactivate="DOTAGameEndContinue()">
			<Label text="#DOTA_Continue" />
		</Button>

		<Label id="MatchSignoutDelayedLabel" text="#dota_game_end_signout_delayed" hittest="false"/>

	</DOTAGameEnd>
</root>


