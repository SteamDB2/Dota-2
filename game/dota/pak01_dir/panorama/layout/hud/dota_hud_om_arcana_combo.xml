<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/hudstyles.vcss_c" />
		<include src="s2r://panorama/styles/hud/dota_hud_om_arcana_combo.vcss_c" />
	</styles>
	<DOTAHUDOMArcanaCombo hittest="false" hittestchildren="false">
			<Panel id="ComboCountContainer">
				<Panel id="ComboDetail">
					<DOTAScenePanel id="ComboAmbientFX" hittest="false" camera="camera" renderdeferred="false" rendershadows="false" particleonly="true" map="scenes/hud/ui_om_arcana_combo_ambient"/>
				</Panel>
				<Panel id="ComboCountContents">
					<Panel id="ComboContain" class="ComboDigits DigitContainer">
						<Panel class="ComboX ComboCountX ComboCountValue"/>
						<Panel id="ComboCountDigit1" class="ComboCountDigit OMDigit ComboCountValue"/>
						<Panel id="ComboCountDigit2" class="ComboCountDigit OMDigit ComboCountValue"/>
						<Panel id="ComboCountDigit3" class="ComboCountDigit OMDigit ComboCountValue"/>
					</Panel>
					<Label class="ComboCountLabel" text="#DOTA_Ogre_Magi_Arcana_Hud_StreakTitle"/>
					<Label class="StreakOddsLabel" text="#DOTA_Ogre_Magi_Arcana_Hud_StreakOdds"/>
				</Panel>
			</Panel>

			<Panel id="ComboSummaryContainer" >
				<DOTAScenePanel id="ComboSummaryFX" hittest="false" camera="camera" renderdeferred="false" rendershadows="false" particleonly="true" map="scenes/hud/ui_om_arcana_combo_summary"/>
				<Panel id="ComboDetail2" />
				<Panel id="ComboSummaryContents">
					<Panel class="ComboSummary">
						<Panel class="DigitContainer">
							<Panel class="ComboX ComboSummaryX"/>
							<Panel id="ComboSummaryDigit1" class="ComboSummaryDigit OMDigit"/>
							<Panel id="ComboSummaryDigit2" class="ComboSummaryDigit OMDigit"/>
							<Panel id="ComboSummaryDigit3" class="ComboSummaryDigit OMDigit"/>
						</Panel>
						<Label class="SummaryComboLabel" text="#DOTA_Ogre_Magi_Arcana_Hud_StreakTitle"/>
					</Panel>

					<Panel class="GemIncrementSummary">
						<Panel  id="GemIncrementContain" class="DigitContainer">
							<Panel id="GemIncrementSummaryDigit3" class="GemIncrementSummaryDigit OMDigit"/>
							<Panel id="GemIncrementSummaryDecimal" class="GemIncrementSummaryDigit OMDecimal"/>
							<Panel id="GemIncrementSummaryDigit1" class="GemIncrementSummaryDigit OMDigit"/>
							<Panel id="GemIncrementSummaryDigit2" class="GemIncrementSummaryDigit OMDigit"/>
						</Panel>
						<Label class="SummaryGemIncrementLabel" text="#DOTA_Ogre_Magi_Arcana_Hud_StreakSummaryPointsLabel"/>
					</Panel>
				</Panel>
			</Panel>

	</DOTAHUDOMArcanaCombo>
</root>
