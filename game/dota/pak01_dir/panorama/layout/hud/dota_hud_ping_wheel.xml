<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/hudstyles.vcss_c" />
		<include src="s2r://panorama/styles/hud/dota_hud_ping_wheel.vcss_c" />
	</styles>
	<DOTAPingWheel class="Hidden" hittest="false">
		<Panel id="Root">
			<Panel id="BG" class="PingWheelBG" />
			<Panel id="Attack" class="PingWheelOption" ping-option="attack" min-distance="35" max-distance="500" start-angle="315" end-angle="45"/>
			<Panel id="Retreat" class="PingWheelOption" ping-option="retreat" min-distance="35" max-distance="500" start-angle="135" end-angle="225"/>
			<Panel id="Waypoint" class="PingWheelOption" ping-option="waypoint" min-distance="35" max-distance="500" start-angle="225" end-angle="315"/>
			<Panel id="EnemyWard" class="PingWheelOption" ping-option="enemyward" min-distance="35" max-distance="500" start-angle="90" end-angle="135"/>
			<Panel id="FriendlyWard" class="PingWheelOption" ping-option="friendlyward" min-distance="35" max-distance="500" start-angle="45" end-angle="90"/>
			<Panel id="DefaultPing" class="PingWheelOption" max-distance="35" ping-option="default" />
		</Panel>
	</DOTAPingWheel>
</root>


