<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/hud/dota_hud_query_unit_stats.vcss_c" />
	</styles>
	<DOTAQueryUnitStats hittest="false">
		<Panel id="Damage">
            <Panel id="DamageIcon" />
            <Label id="DamageLabel" class="StatLabel" text="{s:damage}" html="true" />
		</Panel>
		<Panel class="QueryStatsFiller" />
		<Panel id="Armor">
			<Panel id="ArmorIcon" />
			<Panel id="ArmorLabelContainer">
				<Label id="ArmorLabel" class="StatLabel" text="{s:armor}" html="true" />
			</Panel>
		</Panel>
		<Panel class="QueryStatsFiller" />
		<Panel id="MoveSpeed">
			<Panel id="MoveSpeedIcon" />
			<Label id="MoveSpeedLabel" class="StatLabel" text="{s:move_speed}" html="true" />
		</Panel>
		<!--
		<Panel id="Evasion">
			<Panel id="EvasionIcon" />
			<Label id="EvasionLabel" text="{s:evasion}" html="true" />
		</Panel>
		-->
	</DOTAQueryUnitStats>
</root>


