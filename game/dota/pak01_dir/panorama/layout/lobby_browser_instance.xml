<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/lobby_browser_instance.vcss_c" />
	</styles>

	<DOTALobbyBrowserInstance onactivate="DOTALobbyRowClicked()" ondblclick="DOTALobbyRowDoubleClicked()">    

		<Panel class="FriendsIcon" />
		<Panel class="LANIcon" />
		<Panel class="LeagueIcon" />
		<Panel class="PasswordIcon" />
		<Label text="#dota_lobby_row_game_name" class="GameNameField LobbyResultField"/>
		<Label text="#dota_lobby_row_slots" class="SlotsField LobbyResultField"/>
		<Label text="#dota_lobby_row_game_mode" class="GameModeField LobbyResultField" id="GameModeField" />
		<Label text="#dota_lobby_row_region" class="RegionField LobbyResultField" id="RegionField" />

	</DOTALobbyBrowserInstance>

</root>
