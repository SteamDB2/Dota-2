<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/tooltips/battle_pass_activities/tooltip_battle_pass_achievements.vcss_c" />
	</styles>

	<Panel>
		<Label class="Title" text="#DOTA_BattlePass_AchievementsTooltip_Title" />

		<Panel class="MainContents">
			<Panel class="SingleStat Completed">
				<Label class="StatValue" text="{d:achievements_completed} / {d:total_achievements}" />
				<Label class="StatLabel" text="#DOTA_BattlePass_AchievementsTooltip_Completed" />
			</Panel>

			<Panel class="SingleStat Earned">
				<Label class="StatValue" text="{d:total_points}" />
				<Label class="StatLabel" text="#DOTA_BattlePass_AchievementsTooltip_BattlePointsEarned" />
			</Panel>
		</Panel>
	</Panel>
</root>
