<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/tooltips/tooltip_high_priority_queue.vcss_c" />
	</styles>

	<Panel class="HighPriorityQueueContents">
		<Label id="HighPriorityEnabledTitle" class="Title" text="#DOTA_RR_RoleQueue" />
		<Label id="HighPriorityDisabledTitle" class="Title" text="#DOTA_RR_NoRoleQueue" />
		<Label id="Description" />

		<Panel id="Body" >
		<Panel id="LeftColumn">
			<Panel class="ProgressBarLabels">
				<Label id="GamesRemaining" />
				<!-- <Panel class="FillWidth" /> -->
			</Panel>
			
			<ProgressBarWithMiddle id="ProgressBar" class="HPQ_Bar" min="0.0" max="1.0">
				<Panel id="ProgressBar_Left">
					<!-- <Panel id="ProgressBar_Left_Cells" /> -->
				</Panel>
				<Panel id="ProgressBar_Middle">
					<Panel id="ProgressBar_Middle_Content">
						<Panel id="ProgressBar_Middle_Scrolly" />
					</Panel>
					<!-- <Panel id="ProgressBar_Middle_Cells" /> -->
				</Panel>
			</ProgressBarWithMiddle>
			<Label id="GamesChange" />
			<Label class="Reason" text="#DOTA_RR_Reason" />
			<Label id="RolePerformanceTitle" class="Title" text="#DOTA_RR_RolePerformanceTitle" />
			<Label id="RolePerformanceExplanation" text="#DOTA_RR_RoleHandicapExplain" />
		</Panel>
		<Panel id="RightColumn">
			<Panel id="GraphContainer">
				<Panel id="GraphContainerSupportSoftHandicap" />
				<Panel id="GraphContainerSupportHardHandicap" />
				<Panel id="GraphContainerMidlaneHandicap" />
				<Panel id="GraphContainerOfflaneHandicap" />
				<Panel id="GraphContainerSafelaneHandicap" />
				
			</Panel>
			<Label id="RankedRolePerformance" text="#DOTA_RR_RoleHandicapTitle"/>
			<Panel id="SoftSupportIcon" class="LaneIcon" />
			<Panel id="HardSupportIcon" class="LaneIcon" />
			<Panel id="MidlaneIcon" class="LaneIcon" />
			<Panel id="OfflaneIcon" class="LaneIcon" />
			<Panel id="SafelaneIcon" class="LaneIcon" />
		</Panel>
		</Panel>
	</Panel>
</root>
