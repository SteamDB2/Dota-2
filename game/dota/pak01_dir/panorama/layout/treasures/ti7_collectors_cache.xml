<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/international2017/bp_international2017_shared.vcss_c" />
		<include src="s2r://panorama/styles/treasures/ti7_collectors_cache.vcss_c" />
	</styles>

	<Panel class="RootPanel">
		<Panel id="PanelHeader">
	
			<Panel id="BattlePassLogo" />
			
			<Label class="HeaderText" text="#DOTA_TI6_CollectorsCache_HeaderText" />
			
		</Panel>

		<Panel id="AchievementProgress">
			
			<Panel id="AchievementTiers">
				<Panel class="AchievementAmount">
					<Label class="AchievementLevel" text="1 =" />
					<Panel class="XPCircle">
						<Label class="AchievementReward" text="2000" />
						<Panel class="PTS" />
					</Panel>
				</Panel>
				<Panel class="AchievementAmount">
					<Label class="AchievementLevel" text="10 =" />
					<Panel class="XPCircle" onmouseout="DOTAHideEconItemTooltip();" onmouseover="DOTAShowEconItemTooltip( 17353, -1, -1 );">
							<Panel class="RedemptionCircle" />
					</Panel>
				
				</Panel>
				<Panel class="AchievementAmount">
					<Label class="AchievementLevel" text="20 =" />
					<Panel class="XPCircle">
						<Label class="AchievementReward" text="20000" />
						<Panel class="PTS" />
					</Panel>
				</Panel>
			</Panel>
			<DOTAEventActionCount eventid="18" actionid="55" layoutfile="file://{resources}/layout/treasures/collectors_cache_2017_counter.xml" setclassidprefix="ProgressTick_" setclass="Ticked"/> 
		</Panel>

		<Label class="FooterText" text="#DOTA_TI6_CollectorsCache_FooterText" />
		
	</Panel>
</root>
